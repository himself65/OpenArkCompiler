                                  from.GetOpCode() == OP_free ||
                                  from.GetOpCode() == OP_decref ||
                                  from.GetOpCode() == OP_incref ||
                                  from.GetOpCode() == OP_decrefreset ||
                                  from.GetOpCode() == OP_assertnonnull ||
