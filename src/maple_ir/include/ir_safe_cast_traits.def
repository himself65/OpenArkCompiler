REGISTER_SAFE_CAST(IfStmtNode, from.GetOpCode() == OP_if);
REGISTER_SAFE_CAST(WhileStmtNode, from.GetOpCode() == OP_while ||
                                  from.GetOpCode() == OP_dowhile);
REGISTER_SAFE_CAST(DoloopNode, from.GetOpCode() == OP_doloop);
